{
  "nodeModulesDir": "auto",
  "unstable": [
    "unsafe-proto",
    "sloppy-imports"
  ],
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "strict": true,
    "jsx": "preserve"
  },
  "fmt": {
    "exclude": [
      "src/app/globals.tailwind.css"
    ]
  },
  "tasks": {
    "dev": "deno run -A --unstable-detect-cjs npm:next@latest dev --turbopack",
    "build": "deno run -A --unstable-detect-cjs npm:next@latest build --turbopack",
    "start": "deno run -A --unstable-detect-cjs npm:next@latest start"
  }
}
